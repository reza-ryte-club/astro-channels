{
  "root": true,
    "rules": {
        "array-bracket-spacing": 2,
        "block-spacing": [2, "never"],
        "brace-style": ["error", "1tbs", { "allowSingleLine": true }],
        "camelcase": [0, {"properties": "always"}],
        "curly": [2, "multi-line"],
        "default-case": 2,
        "dot-notation": 2,
        "eqeqeq": 2,
        "indent": [2, 2, { "SwitchCase": 1,"VariableDeclarator": 1 }],
        "key-spacing": [2, {"beforeColon": false, "afterColon": true}],
        "max-len": [2, 90, 2, {"ignoreUrls": true}],
        "no-extra-bind": 2,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-unused-labels": 2,
        "no-unused-expressions": [0, { "allowShortCircuit": true, "allowTernary": true }],
        "new-cap": 2,
        "no-self-assign" : 2,
        "no-console": 2,
        "no-alert": 2,
        "no-empty": 2,
        "no-extra-boolean-cast": 0,
        "consistent-return": 1,
        "no-func-assign": 2,
        "no-duplicate-case": 2,
        "no-else-return": 2,
        "no-eval": 2,
        "no-multi-spaces": 2,
        "no-multiple-empty-lines": [2, {"max": 2}],
        "no-shadow": 0,
        "no-trailing-spaces": 2,
        "no-unused-vars": [2, {"args": "none"}],
        "object-curly-spacing": [2, "always", {"objectsInObjects": false, "arraysInObjects": false}],
        "padded-blocks": [2, "never"],
        "no-octal": 0,
        "quotes": [
            1,
            "single", {
             "allowTemplateLiterals" : true
             }
        ],
        "semi": [
            2,
            "always"
        ],

        "space-before-blocks": 2,
        "space-before-function-paren": [0, "never"],
        "spaced-comment": 2,
        "valid-typeof": 2
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jquery": true
    },
    "extends": "eslint:recommended",
  "globals": {
      "getJSON": true,
      "postJSON": true,
      "swal": true,
      "db": true,
      "multer": true,
      "multerstorages3": true,
      "app": true,
      "passport": true,
      "io": true,
      "http": true
  }}
